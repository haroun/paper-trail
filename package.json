{
  "name": "paper-trail",
  "version": "0.0.1",
  "description": "Follow the paper-trail",
  "main": "index.js",
  "scripts": {
    "lint": "xo",
    "test": "tape '{,!(node_modules)/**/}/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haroun/paper-trail.git"
  },
  "keywords": [
    "paper-trail",
    "finance",
    "money"
  ],
  "author": "Harouna Traore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haroun/paper-trail/issues"
  },
  "homepage": "https://github.com/haroun/paper-trail#readme",
  "devDependencies": {
    "faucet": "0.0.1",
    "tape": "^4.9.1",
    "xo": "^0.21.1"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before",
            "||": "before"
          }
        }
      ]
    }
  },
  "dependencies": {
    "cuid": "^2.1.1"
  }
}
